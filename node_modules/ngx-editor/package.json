{
  "name": "ngx-editor",
  "version": "15.3.0",
  "description": "The Rich Text Editor for Angular, Built on ProseMirror",
  "license": "MIT",
  "repository": "https://github.com/sibiraj-s/ngx-editor.git",
  "bugs": "https://github.com/sibiraj-s/ngx-editor/issues",
  "homepage": "https://github.com/sibiraj-s/ngx-editor",
  "keywords": [
    "angular-editor",
    "angular-wysiwyg-editor",
    "wysiwyg-editor",
    "ngx-editor",
    "prosemirror"
  ],
  "peerDependencies": {
    "@angular/common": ">=14.1.0",
    "@angular/core": ">=14.1.0",
    "@angular/forms": ">=14.1.0",
    "@angular/platform-browser": ">=14.1.0",
    "rxjs": ">=7.5.0"
  },
  "dependencies": {
    "@floating-ui/core": "1.2.0",
    "@floating-ui/dom": "1.2.0",
    "prosemirror-commands": "1.5.0",
    "prosemirror-history": "1.3.0",
    "prosemirror-inputrules": "1.2.0",
    "prosemirror-keymap": "1.2.0",
    "prosemirror-model": "1.19.0",
    "prosemirror-schema-list": "1.2.2",
    "prosemirror-state": "1.4.2",
    "prosemirror-view": "1.30.1",
    "tslib": "^2.3.0"
  },
  "module": "fesm2015/ngx-editor.mjs",
  "es2020": "fesm2020/ngx-editor.mjs",
  "esm2020": "esm2020/ngx-editor.mjs",
  "fesm2020": "fesm2020/ngx-editor.mjs",
  "fesm2015": "fesm2015/ngx-editor.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ngx-editor.mjs",
      "es2020": "./fesm2020/ngx-editor.mjs",
      "es2015": "./fesm2015/ngx-editor.mjs",
      "node": "./fesm2015/ngx-editor.mjs",
      "default": "./fesm2020/ngx-editor.mjs"
    },
    "./commands": {
      "types": "./commands/index.d.ts",
      "esm2020": "./esm2020/commands/ngx-editor-commands.mjs",
      "es2020": "./fesm2020/ngx-editor-commands.mjs",
      "es2015": "./fesm2015/ngx-editor-commands.mjs",
      "node": "./fesm2015/ngx-editor-commands.mjs",
      "default": "./fesm2020/ngx-editor-commands.mjs"
    },
    "./helpers": {
      "types": "./helpers/index.d.ts",
      "esm2020": "./esm2020/helpers/ngx-editor-helpers.mjs",
      "es2020": "./fesm2020/ngx-editor-helpers.mjs",
      "es2015": "./fesm2015/ngx-editor-helpers.mjs",
      "node": "./fesm2015/ngx-editor-helpers.mjs",
      "default": "./fesm2020/ngx-editor-helpers.mjs"
    },
    "./input-rules": {
      "types": "./input-rules/index.d.ts",
      "esm2020": "./esm2020/input-rules/ngx-editor-input-rules.mjs",
      "es2020": "./fesm2020/ngx-editor-input-rules.mjs",
      "es2015": "./fesm2015/ngx-editor-input-rules.mjs",
      "node": "./fesm2015/ngx-editor-input-rules.mjs",
      "default": "./fesm2020/ngx-editor-input-rules.mjs"
    },
    "./schema": {
      "types": "./schema/index.d.ts",
      "esm2020": "./esm2020/schema/ngx-editor-schema.mjs",
      "es2020": "./fesm2020/ngx-editor-schema.mjs",
      "es2015": "./fesm2015/ngx-editor-schema.mjs",
      "node": "./fesm2015/ngx-editor-schema.mjs",
      "default": "./fesm2020/ngx-editor-schema.mjs"
    },
    "./utils": {
      "types": "./utils/index.d.ts",
      "esm2020": "./esm2020/utils/ngx-editor-utils.mjs",
      "es2020": "./fesm2020/ngx-editor-utils.mjs",
      "es2015": "./fesm2015/ngx-editor-utils.mjs",
      "node": "./fesm2015/ngx-editor-utils.mjs",
      "default": "./fesm2020/ngx-editor-utils.mjs"
    }
  },
  "sideEffects": false
}